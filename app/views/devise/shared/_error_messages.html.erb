<% if resource.errors.any? %>
<div id="error_explanation">
  <% resource.errors.full_messages.each do |message| %>
  <div class="alert alert-dismissible alert-danger">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= message %>
  </div>
  <% end %>
</div>
<% end %>